{
  "presets": [
    "es2015-rollup",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "async-import",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap",
            "ignoreFilenames": [
              "node_modules"
            ]
          }
        ]
      ]
    }
  }
}